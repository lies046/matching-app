<div class="container">
  <div class="row">
  <% @matches.each do |match| %>
    <div class="col-sm-4">
    <% if match.images.attached? %>
      <div class="match-tile rounded" data-id="<%= match.id %>"style="background-image: url('<%= url_for match.images.first %>')">
      <p class="text-white"><%= match.first_name %></p>
    <% end %>
    </div>
  <% end %>
  </div>
</div>